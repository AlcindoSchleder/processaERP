	CadArqSql_SqlOwnerUpdateIm,	"update CADASTROS_IMAGENS set \n\r       FLAG_TIMG    = :FlagTImg, \n\r       IMG_CAD      = :ImgCad \n\r where FK_CADASTROS = :FkCadastros"
