	SqlComm_SqlParamEmp,	"select * from VW_EMPRESAS_PARAM where FK_EMPRESAS = :Empresa"
	SqlComm_SqlAutorizar,	"select * from OPERADORES where PK_OPERADORES = :PkOperadores"
	SqlComm_SqlGravaHistorico,	"execute procedure GRAVA_HISTORICOS \n\r  (:CODEMPR, :NOMOPE, :NOMFORM, :DSCOPE, :CODOPE, :NOMARQ)"
	SqlComm_SqlDictionary,	"select * from DICIONARIOS              \n\r where FK_LINGUAGENS      = :Linguagem \n\r   and PK_DICIONARIOS__NA = :NameFile  \n\r order by POS_FLD"
	SqlComm_SqlDicField,	"select * from DICIONARIOS              \n\r where FK_LINGUAGENS      = :Linguagem \n\r   and PK_DICIONARIOS__NA = :NameFile  \n\r   and PK_DICIONARIOS__NC = :NameField \n\r order by POS_FLD"
END

