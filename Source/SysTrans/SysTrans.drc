	ArqSqlTrans_SqlInsertManifest,	"insert into MANIFESTOS \n\r  (FK_EMPRESAS, PK_MANIFESTOS, FK_TIPO_MANIFESTOS, \n\r   FK_VEICULOS_MARCAS, FK_VEICULOS_MODELOS, \n\r   FK_VEICULOS, FK_FUNCIONARIOS__MOTORISTA, \n\r   FK_AGREGADO, FK_FUNCIONARIOS__CARGA, \n\r   FK_FUNCIONARIOS__CONFERENCIA, DTA_EMI, DTA_SAI, \n\r   FLAG_TOT, TOT_CNH, VLR_PDG, VLR_MRC, VLR_FRT_VST, \n\r   VLR_REMB) \n\rvalues \n\r  (:FkEmpresas, :PkManifestos, :FkTipoManifestos, \n\r   :FkVeiculosMarcas, :FkVeiculosModelos, \n\r   :FkVeiculos, :FkFuncionariosMotorista, \n\r   :FkAgregado, :FkFuncionariosCarga, \n\r   :FkFuncionariosConferencia, :DtaEmi, :DtaSai, \n\r   :FlagTot, :TotCnh, :VlrPdg, :VlrMrc, VlrFrtVst, \n\r   :VlrRemb)"
	ArqSqlTrans_SqlUpdateManifest,	"update MANIFESTOS set \n\r       FK_TIPO_MANIFESTOS           = :FkTipoManifestos, \n\r       FK_VEICULOS_MARCAS           = :FkVeiculosMarcas, \n\r       FK_VEICULOS_MODELOS          = :FkVeiculosModelos, \n\r       FK_VEICULOS                  = :FkVeiculos, \n\r       FK_FUNCIONARIOS__MOTORISTA   = :FkFuncionariosMotorista, \n\r       FK_AGREGADO                  = :FkAgregado, \n\r       FK_FUNCIONARIOS__CARGA       = :FkFuncionariosCarga, \n\r       FK_FUNCIONARIOS__CONFERENCIA = :FkFuncionariosConferencia, \n\r       DTA_EMI                      = :DtaEmi, \n\r       DTA_SAI                      = :DtaSai, \n\r       FLAG_TOT                     = :FlagTot, \n\r       TOT_CNH                      = :TotCnh, \n\r       VLR_PDG                      = :VlrPdg, \n\r       VLR_MRC                      = :VlrMrc, \n\r       VLR_FRT_VST                  = :VlrFrtVst, \n\r       VLR_REMB                     = :VlrRemb \n\r where FK_EMPRESAS                  = :FkEmpresas \n\r       PK_MANIFESTOS                = :PkManifestos"
	ArqSqlTrans_SqlManifestDocs,	"select Mch.*, Dch.NUM_NF, Dch.FLAG_REMB, Dch.FLAG_TP_FRE, \n\r       Doc.DATA_DOC, Org.RAZ_SOC as DSC_ORGM, \n\r       Dst.RAZ_SOC as DSC_DSTN, Dch.VLR_FRE \n\r  from MANIFESTOS_CONHECIMENTOS Mch, DOCUMENTOS_CONHECIMENTOS Dch, \n\r       DOCUMENTOS Doc, CADASTROS Org, CADASTROS Dst \n\r where Mch.FK_EMPRESAS        = :FkEmpresas \n\r   and Mch.FK_MANIFESTOS      = :FkManifestos \n\r   and Doc.FK_EMPRESAS        = Mch.FK_EMPRESAS \n\r   and Doc.FK_TIPO_DOCUMENTOS = Mch.FK_TIPO_DOCUMENTOS \n\r   and Doc.FK_CADASTROS       = Mch.FK_CADASTROS \n\r   and Doc.PK_DOCUMENTOS      = Mch.FK_DOCUMENTOS \n\r   and Dch.FK_EMPRESAS        = Mch.FK_EMPRESAS \n\r   and Dch.FK_TIPO_DOCUMENTOS = Mch.FK_TIPO_DOCUMENTOS \n\r   and Dch.FK_CADASTROS       = Mch.FK_CADASTROS \n\r   and Dch.FK_DOCUMENTOS      = Mch.FK_DOCUMENTOS \n\r   and Org.PK_CADASTROS       = Dch.FK_REMETENTE \n\r   and Dst.PK_CADASTROS       = Dch.FK_DESTINATARIO \n\r order by Mch.FK_TIPO_DOCUMENTOS, Mch.FK_CADASTROS, Mch.FK_DOCUMENTOS"
