	ArqSqlTrans_SqlSearchCarrierDocs,	"select Dch.NUM_NF, Dch.FLAG_REMB, Dch.FLAG_TP_FRE, \n\r       Doc.DATA_DOC, Org.RAZ_SOC as DSC_ORGM, \n\r       Dst.RAZ_SOC as DSC_DSTN, Dch.VLR_FRE \n\r  from DOCUMENTOS_CONHECIMENTOS Dch, \n\r       DOCUMENTOS Doc, CADASTROS Org, CADASTROS Dst \n\r where Dch.FK_EMPRESAS        = :FkEmpresas \n\r   and ((Dch.FK_DOCUMENTOS    = :FkDocumentos) \n\r    or ( 0                    = :FkDocumentos)) \n\r   and ((Dhc.NUM_NF           = :NumNf) \n\r    or ( 0                    = :NumNf) \n\r   and Org.PK_CADASTROS       = Dch.FK_REMETENTE \n\r   and Dst.PK_CADASTROS       = Dch.FK_DESTINATARIO \n\r   and Doc.FK_EMPRESAS        = Dch.FK_EMPRESAS \n\r   and Doc.FK_TIPO_DOCUMENTOS = Dch.FK_TIPO_DOCUMENTOS \n\r   and Doc.FK_CADASTROS       = Dch.FK_CADASTROS \n\r   and Doc.PK_DOCUMENTOS      = Dch.FK_DOCUMENTOS \n\r   and ((Doc.DATA_DOC        >= :StartDate) \n\r    or ( Cast(0 as Date)      = :StartDate)) \n\r   and ((Doc.DATA_DOC        >= :EndDate) \n\r    or ( Cast(0 as Date)      = :EndDate)) \n\r order by Doc.DATA_DOC, Doc.PK_DOCUMENTOS"
