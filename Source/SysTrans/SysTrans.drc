	ArqSqlTrans_SqlSelectAllVehicles1,	"select Cast(0 as integer) as PK_TYPE_VEHICLE, \n\r       Cast('Veículos Próprios' as varchar(30)) as DSC_TVEI, \n\r       Pvl.FK_PRODUTOS as PK_OWNER, Mrc.PK_VEICULOS_MARCAS, \n\r       Mrc.DSC_MRC, Mod.PK_VEICULOS_MODELOS, \n\r       Mod.DSC_MOD, Vei.PK_VEICULOS, Prd.DSC_PROD as DSC_VEI \n\r  from PARAMETROS_GLOBAIS Pgb \n\r  left outer join VEICULOS_MARCAS Mrc \n\r    left outer join VEICULOS_MODELOS Mod \n\r      left join PATRIMONIO_VEICULOS Pvl \n\r        join VEICULOS Vei \n\r          on Vei.FK_VEICULOS_MARCAS  = Pvl.FK_VEICULOS_MARCAS \n\r         and Vei.FK_VEICULOS_MODELOS = Pvl.FK_VEICULOS_MODELOS \n\r        join PRODUTOS Prd \n\r          on Prd.PK_PRODUTOS         = Pvl.FK_PRODUTOS \n\r        on Pvl.FK_VEICULOS_MARCAS  = Mrc.PK_VEICULOS_MARCAS \n\r       and Pvl.FK_VEICULOS_MODELOS = Mod.PK_VEICULOS_MODELOS \n\r       and Vei.PK_VEICULOS         = Pvl.FK_VEICULOS \n\r      on Mod.FK_VEICULOS_MARCAS  = Mrc.PK_VEICULOS_MARCAS \n\r    on Mrc.PK_VEICULOS_MARCAS  > 0 \n\r where Pgb.PK_PARAMETROS_GLOBAIS = 1 "
